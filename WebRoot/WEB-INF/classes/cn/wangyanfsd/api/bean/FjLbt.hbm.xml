<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.wangyanfsd.api.bean">
	<class name="FjLbt" table="tb_FjLbt">
		<id name="Id" column="Id" type="java.lang.Integer">
			<generator class="native"></generator>  
		</id>
		<property name="title" column="title" type="java.lang.String" length="20"/>
		<property name="content" column="content" type="text" />
		<property name="date" column="date" type="java.lang.String" length="20"/>
		<property name="person" column="person" type="java.lang.String" length="20"/>
	</class>
</hibernate-mapping>
